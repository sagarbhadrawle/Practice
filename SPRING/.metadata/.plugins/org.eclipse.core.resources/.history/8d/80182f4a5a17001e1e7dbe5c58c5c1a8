package com;
import java.util.*;

public class Demo {
    public static void main(String[] args) {
        
    	Map<Integer, String> map = new LinkedHashMap<>();

		map.put(1, "one");
		map.put(2, "two");
		map.put(3, "three");
		map.put(4, "four");
		map.put(5, "five");
		map.put(6, "six");

		System.out.println(map);
		
		Set<Integer> keys  = map.keySet();
		System.out.println(keys);
    	
    }
}


















